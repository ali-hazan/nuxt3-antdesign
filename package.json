{
  "name": "nuxt-antdesign",
  "version": "0.1.0",
  "description": "Nuxt3 Starter with AntDesign",
  "private": true,
  "engines": {
    "node": ">=16.15.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "storybook": "start-storybook -p 6006",
    "preview": "nuxt preview",
    "format": "prettier --cache --write \"**/*.{cjs,js,jsx,ts,tsx,vue}\"",
    "lint": "eslint . --fix --ext .cjs,.js,.jsx,.ts,.tsx,.vue",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check:all": "yarn format && yarn lint && yarn coverage"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.3.0-beta.4",
    "pinia": "^2.0.24",
    "swiper": "^8.4.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.31.0",
    "@fontsource/fira-mono": "^4.5.10",
    "@fontsource/inter": "^4.5.14",
    "@fontsource/poppins": "^4.5.10",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@nuxt/test-utils-edge": "^3.0.1-rc.0-27810184.d991a55",
    "@nuxtjs/i18n": "^8.0.0-beta.7",
    "@pinia/nuxt": "^0.4.4",
    "@pinia/testing": "^0.0.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/vue3": "^6.5.14",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitest/coverage-c8": "^0.25.2",
    "@vueuse/nuxt": "^9.5.0",
    "c8": "^7.12.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^9.7.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "nuxt": "~3.0.0",
    "nuxt-server-fn": "^0.3.0",
    "postcss": "^8.4.19",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.7.1",
    "storybook-builder-vite": "^0.1.23",
    "typescript": "~4.9.3",
    "unplugin-vue-components": "^0.22.12",
    "vitest": "^0.25.2"
  }
